REPTIDNEST -> dot id IDNESTTAIL REPTIDNEST .
REPTIDNEST -> EPSILON .

CONTINUATION -> REPTIDNEST id FACTORTAIL .

FACTORTAIL -> REPTINDICE .
FACTORTAIL -> PARAMS .

FUNCHEAD -> function FUNCHEADTAIL

FUNCHEADTAIL -> id FUNCHEADTAILTAIL .

FUNCHEADTAILTAIL -> sr FUNCHEADTAILTAILTAIL .
FUNCHEADTAILTAIL -> lpar FPARAMS rpar arrow RETURNTYPE .

FUNCHEADTAILTAILTAIL -> id lpar FPARAMS rpar arrow RETURNTYPE .
FUNCHEADTAILTAILTAIL -> constructor lpar FPARAMS rpar .

LOCALVARDECLTAIL -> REPTARRAYSIZE semi .
LOCALVARDECLTAIL -> ENDPARAMS .

MEMBERVARDECL -> attribute id colon TYPE REPTARRAYSIZE semi .

REPTARRAYSIZE -> ARRAYSIZE REPTARRAYSIZE .
REPTARRAYSIZE -> EPSILON .

STATEMENTTAIL -> INDICE REPTINDICE STATEMENTBRACK .
STATEMENTTAIL -> ASSIGNEND .
STATEMENTTAIL -> POINTNESTR STATEMENTPOINTNEST .
STATEMENTTAIL -> PARAMS STATEMENTPARENT .

STATEMENTPARENT -> POINTNESTR STATEMENTPOINTNEST .
STATEMENTPARENT -> semi .

STATEMENTPOINTNEST -> REPTINDICE ASSIGNEND .
STATEMENTPOINTNEST -> ENDPARAMS .

STATEMENTBRACK -> ASSIGNEND .
STATEMENTBRACK -> POINTNESTR STATEMENTPOINTNEST .

ENDPARAMS -> PARAMS semi .

VARIABLETAIL -> INDICE REPTINDICE VARIABLEBRACK .
VARIABLETAIL -> EPSILON .
VARIABLETAIL -> POINTNESTR REPTINDICE .
VARIABLETAIL -> PARAMS POINTNESTR REPTINDICE .

REPTINDICE -> INDICE REPTINDICE .
REPTINDICE -> EPSILON .

POINTNESTR -> dot REPTIDNESTR id .

REPTIDNESTR -> id REPTIDNESTRTAIL .
REPTIDNESTRRTAIL -> EPSILON .
REPTIDNESTRRTAIL -> INDICE REPTINDICE POINTNESTR .
REPTIDNESTRRTAIL -> POINTNESTR .
REPTIDNESTRRTAIL -> PARAMS POINTNESTR .

IDNESTTAIL -> INDICE REPTINDICE dot .
IDNESTTAIL -> dot .
IDNESTTAIL -> PARAMS dot .